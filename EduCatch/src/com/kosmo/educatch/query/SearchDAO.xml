<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kosmo.educatch.dao.SearchMapper">
	<!-- 지역data -->
	<select id="areaData" parameterType="Search" resultMap="Search">
	SELECT 
	        A.DISTRICT    AS DISTRICT,
	        A.CITY       AS CITY
	FROM    AREA A
	ORDER BY 1 DESC;
	</select>
	<!-- 분야data -->
	<select id="categoryData" parameterType="Search" resultMap="Search">
		SELECT 
		        A.CNO       AS CNO,
		        A.CMAJOR    AS CMAJOR,
		        A.CMINOR    AS CMINOR
		FROM    CATEGORY A
		ORDER BY 1 DESC;
	</select>
</mapper>